{
  "name": "follicle-labeller",
  "version": "1.2.1",
  "description": "Medical image follicle annotation tool",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/blockchainadvisors/follicle-labeller.git"
  },
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "icons": "node scripts/generate-icons.js",
    "version:sync": "node scripts/sync-version.js",
    "electron:dev": "vite",
    "electron:build": "npm run version:sync && npm run build && electron-builder",
    "electron:build:win": "npm run version:sync && npm run build && electron-builder --win",
    "electron:build:mac": "npm run version:sync && npm run build && electron-builder --mac",
    "electron:build:all": "npm run version:sync && npm run build && electron-builder --win --mac",
    "test:benchmark": "tsx test/blob-detection-benchmark/index.ts"
  },
  "dependencies": {
    "electron-updater": "^6.1.7",
    "jszip": "^3.10.1",
    "lucide-react": "^0.469.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0",
    "zundo": "^2.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "icns-lib": "^1.0.1",
    "png-to-ico": "^3.0.1",
    "sharp": "^0.34.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  }
}
